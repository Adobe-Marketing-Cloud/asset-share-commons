<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,core.wcm.components.page.v1.editor,cq.wcm.msm.properties]"
    helpPath="https://www.adobe.com/go/aem_cmp_page_v1"
    mode="edit">
    <content
        granite:class="cq-dialog-content-page"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="cq-siteadmin-admin-properties-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <asset-share-commons
                        jcr:primaryType="nt:unstructured"
                        sling:orderBefore="basic"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/asset-share-commons/components/structure/search-page/cq:dialog/asset-share-commons"/>
                </items>
            </tabs>
        </items>
    </content>
    <asset-share-commons
        cq:showOnCreate="{Boolean}false"
        jcr:primaryType="nt:unstructured"
        jcr:title="Asset Share"
        sling:orderBefore="basic"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <asset-details
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/asset-share-commons/components/structure/search-page/cq:dialog/asset-share-commons/asset-details"/>
                    <actions
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/asset-share-commons/components/structure/search-page/cq:dialog/asset-share-commons/actions"/>
                    <messages
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/asset-share-commons/components/structure/search-page/cq:dialog/asset-share-commons/messages"/>
                </items>
            </column>
        </items>
        <messages
            jcr:primaryType="nt:unstructured"
            jcr:title="Messages"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
            <items jcr:primaryType="nt:unstructured">
                <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                    <items jcr:primaryType="nt:unstructured">
                        <fieldset
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Messages Configuration"
                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                            <items jcr:primaryType="nt:unstructured">
                                <cart-add-fieldset
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Asset Added to Cart"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                    <items jcr:primaryType="nt:unstructured">
                                        <event-id
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                            name="./config/messages/cart-add/eventId"
                                            value="cart-add"/>
                                        <style
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldLabel="Message Style"
                                            name="./config/messages/cart-add/style">
                                            <items jcr:primaryType="nt:unstructured">
                                                <success
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Success"
                                                    value="success"/>
                                                <info
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Info"
                                                    value="info"/>
                                                <warn
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Warn"
                                                    value="warn"/>
                                                <error
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Error"
                                                    value="error"/>
                                            </items>
                                        </style>
                                        <text
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Message Text"
                                            name="./config/messages/cart-add/text"/>
                                    </items>
                                </cart-add-fieldset>
                                <cart-remove-fieldset
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Asset Removed from Cart"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                    <items jcr:primaryType="nt:unstructured">
                                        <event-id
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                            name="./config/messages/cart-remove/eventId"
                                            value="cart-remove"/>
                                        <style
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldLabel="Message Style"
                                            name="./config/messages/cart-remove/style">
                                            <items jcr:primaryType="nt:unstructured">
                                                <success
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Success"
                                                    value="success"/>
                                                <notice
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Notice"
                                                    value="notice"/>
                                                <warn
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Warn"
                                                    value="warn"/>
                                                <error
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Error"
                                                    value="error"/>
                                            </items>
                                        </style>
                                        <text
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Message Text"
                                            name="./config/messages/cart-remove/text"/>
                                    </items>
                                </cart-remove-fieldset>
                                <download-add-fieldset
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Archive Added To Downloads"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                    <items jcr:primaryType="nt:unstructured">
                                        <event-id
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                            name="./config/messages/download-add/eventId"
                                            value="download-add"/>
                                        <style
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldLabel="Message Style"
                                            name="./config/messages/download-add/style">
                                            <items jcr:primaryType="nt:unstructured">
                                                <success
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Success"
                                                    value="success"/>
                                                <info
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Info"
                                                    value="info"/>
                                                <warn
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Warn"
                                                    value="warn"/>
                                                <error
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Error"
                                                    value="error"/>
                                            </items>
                                        </style>
                                        <text
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Message Text"
                                            name="./config/messages/download-add/text"/>
                                    </items>
                                </download-add-fieldset>
                            </items>
                        </fieldset>
                    </items>
                </column>
            </items>
        </messages>
        <actions
            jcr:primaryType="nt:unstructured"
            jcr:title="Actions Configuration"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
            <items jcr:primaryType="nt:unstructured">
                <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                    <items jcr:primaryType="nt:unstructured">
                        <fieldset
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Actions Configuration"
                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                            <items jcr:primaryType="nt:unstructured">
                                <download-enabled
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                    fieldLabel="Enable Downloading"
                                    name="./config/actions/download/enabled">
                                    <items jcr:primaryType="nt:unstructured">
                                        <always
                                            jcr:primaryType="nt:unstructured"
                                            text="Always"
                                            value="true"/>
                                        <never
                                            jcr:primaryType="nt:unstructured"
                                            text="Never"
                                            value="false"/>
                                        <cart
                                            jcr:primaryType="nt:unstructured"
                                            text="Cart Only"
                                            value="cart"/>
                                    </items>
                                </download-enabled>
                                <download-original
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    name="./config/actions/original-only/enabled"
                                    text="Download Original Rendition Only And Skip Download Options"
                                    uncheckedValue="false"
                                    value="true"/>
                                <download
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldDescription="Path to download action page."
                                    fieldLabel="Download"
                                    name="./config/actions/download/path"
                                    rootPath="/content"/>
                                <license-enabled
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    name="./config/actions/license/enabled"
                                    text="Enable License Management"
                                    uncheckedValue="false"
                                    value="true"/>
                                <license
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldDescription="Path to license agreement action page."
                                    fieldLabel="License Agreement"
                                    name="./config/actions/license/path"
                                    rootPath="/content"/>
                                <share-enabled
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                    fieldLabel="Enable Sharing"
                                    name="./config/actions/share/enabled">
                                    <items jcr:primaryType="nt:unstructured">
                                        <always
                                            jcr:primaryType="nt:unstructured"
                                            text="Always"
                                            value="true"/>
                                        <never
                                            jcr:primaryType="nt:unstructured"
                                            text="Never"
                                            value="false"/>
                                        <cart
                                            jcr:primaryType="nt:unstructured"
                                            text="Cart Only"
                                            value="cart"/>
                                    </items>
                                </share-enabled>
                                <share
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldDescription="Path to share action page."
                                    fieldLabel="Share"
                                    name="./config/actions/share/path"
                                    rootPath="/content"/>
                                <cart-enabled
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    name="./config/actions/cart/enabled"
                                    text="Enable the Cart"
                                    uncheckedValue="false"
                                    value="true"/>
                                <cart
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldDescription="Path to cart action page."
                                    fieldLabel="Cart"
                                    name="./config/actions/cart/path"
                                    rootPath="/content"/>
                                <downloads-panel-enabled
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    name="./config/actions/downloads-panel/enabled"
                                    text="Enable Downloads Panel"
                                    uncheckedValue="false"
                                    value="true"/>
                                <downloads-panel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldDescription="Path to downloads panel action page."
                                    fieldLabel="Downloads Panel"
                                    name="./config/actions/downloads-panel/path"
                                    rootPath="/content"/>
                            </items>
                        </fieldset>
                    </items>
                </column>
            </items>
        </actions>
        <asset-details
            jcr:primaryType="nt:unstructured"
            jcr:title="Asset Details Configuration"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
            <items jcr:primaryType="nt:unstructured">
                <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                    <items jcr:primaryType="nt:unstructured">
                        <fieldset
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Asset Details Configuration"
                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                            <items jcr:primaryType="nt:unstructured">
                                <asset-details-selector
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                    emptyText="Choose an asset details page selector"
                                    fieldLabel="Asset Details Page Selector"
                                    name="./config/asset-details/selector"
                                    required="{Boolean}true">
                                    <datasource
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="asset-share-commons/data-sources/asset-details-selectors"/>
                                </asset-details-selector>
                                <default-asset-details-path
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldDescription="Path to the default details page for displaying asset details. This is used if no Asset Details Page Selector can accept the asset."
                                    fieldLabel="Default Asset Details Page"
                                    name="./config/asset-details/defaultPath"
                                    required="true"
                                    rootPath="/content"/>
                                <asset-details-asset-reference-by-id
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    fieldDescription="Reference assets on asset details pages by ID, rather than path. When unchecked, the asset path is used."
                                    name="./config/asset-details/assetReferenceById"
                                    text="Reference assets by ID"
                                    uncheckedValue="{Boolean}false"
                                    value="{Boolean}true"/>
                                <placeholder-asset-path
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldDescription="Path to the default asset details placeholder image. Placeholder assets are only to aid in authoring."
                                    fieldLabel="Placeholder Asset"
                                    name="./config/asset-details/placeholderPath"
                                    required="true"
                                    rootPath="/"/>
                            </items>
                        </fieldset>
                    </items>
                </column>
            </items>
        </asset-details>
    </asset-share-commons>
</jcr:root>
